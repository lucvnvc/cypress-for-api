{
  "name": "api-with-cypress",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "json-server --watch data.json",
    "open": "cypress open",
    "test": "cypress run",
    "delete:reports": "rm ./results/* || true",
    "test:mocha-reporter": "yarn test --reporter mochawesome",
    "merge-report": "mochawesome-merge \"./results/**.json\" > index.json",
