  "dependencies": {
    "cypress": "^12.5.1",
    "json-server": "0.17.2",
    "typescript": "4.9.5",
    "yarn": "1.22.19"
  },
  "devDependencies": {
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.2",
    "mochawesome-report-generator": "^6.2.0"
  }
}
